{
    "manifest_version": 2,

    "name": "Y U NO SI?!",
    "description": "This extension converts and rewrites all mentions of imperial units to SI",
    "version": "0.1",
    "offline_enabled": true,

    "permissions": [
        "tabs"
        ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "yunosi.html"
    }
}
